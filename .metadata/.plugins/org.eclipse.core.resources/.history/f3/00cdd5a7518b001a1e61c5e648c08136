
//Max of 3 numbers find out which one is greater

package conditionalStatements;
import java.util.Scanner;
public class NestedifElse {

	public static void main(String[] args) {
		System.out.println();
		
		Scanner sc = new Scanner(System.in);
		
		int  a= sc.nextInt();
		int  b= sc.nextInt();
		int  c= sc.nextInt();
		int result;
		
		if(a>b) {
			result = a;
			System.out.println("Here a is greater than b now lets check with the C" +result);
		}
		if(a>c) {
			
			result = c;
			System.out.println("Again A is greater than C");
		}
		
		else {
			
			System.out.println("Please check out with the another condition");
			
		}
	}

}
