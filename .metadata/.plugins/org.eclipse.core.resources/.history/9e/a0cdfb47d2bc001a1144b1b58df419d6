package Arrays;

public class testingRotation {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[]= new int [] {1,2,3,4,5,6,7};
		int n = 7;
		System.out.println("The original array is: ");
		for(int i=0; i<n; i++) {
			System.out.println(arr[i]+" ");
		}
	}

}
