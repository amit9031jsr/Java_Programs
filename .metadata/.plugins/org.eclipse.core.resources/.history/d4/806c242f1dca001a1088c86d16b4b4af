package arrayPrograms;

public class Program1 {
	//In this program we should copy all the elements of array1 to the another array which is array2.
	
	public void copy(int arr1[], int arr2[]) {
		
		
		
		for(int i=0; i<arr1.length; i++) {
			arr2[i]= arr1[i];
			
			System.out.print(arr2[i]);
		}
	}
	
//	public void print1(int arr1[]) {
//		for(int i=0; i<arr1.length; i++) {
//			System.out.print(arr1[i]+ "  ");
//		}
//		System.out.println();
//	}

	public void print2(int arr2[]) {
		for(int i=0; i<arr2.length; i++) {
			System.out.print(arr2[i] + "   ");
		}
		System.out.println();
	}
	public static void main(String[] args) {
		
		Program1 copy = new Program1();
		
		int arr1[]= {1,2,3,4,5};
		int arr2[]= new int[arr1.length];
		
//		copy.print1(arr1);
		copy.print2(arr2);
		
		
		

	}

}
