package Arrays;
import java.util.Scanner;

public class testing {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		int[] marks = new int[n];
		
		for(int i=0; i<n; i++) {
			System.out.println("The marks are as follows" + marks);
		}
		
		

	}

}
